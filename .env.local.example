# Local Development Environment Variables
# 
# This file shows example values for local development and testing.
# Copy this file to `.env.local` to override default .env values.
# 
# NOTE: .env.local is in .gitignore and will NOT be committed.
# This allows you to test features locally without affecting CI/CD.

# Enable features for testing
VITE_FEATURE_DEBUG_PANEL=true
VITE_FEATURE_FLOOR_PLAN=true

# Enable all feature flags for full testing
# VITE_FEATURE_HA_CONNECTION=true
# VITE_FEATURE_OVERLAYS=true
# VITE_FEATURE_ONBOARDING=true

# Enable all overlays
# VITE_OVERLAY_LIGHTING=true
# VITE_OVERLAY_CLIMATE=true
# VITE_OVERLAY_SURVEILLANCE=true
# VITE_OVERLAY_AV=true
# VITE_OVERLAY_NETWORK=true
